package glib {

    gtype GList;
    gtype GHashTable;

}

package demo {

    genum AnimalCatg {

        code BIRD;
        code MAMMAL;

    }

    gobject Animal {

        abstract;

        property category {
            type: enumeration;
            gtype: gtypename( AnimalCatg );
            access: initial-write;
        }

    }

    gobject Bird {

        super Animal;

        constructor {

            init_properties {
                category: AnimalCatg.BIRD;
            }

        }

        method fly {
            visibility: public;
        }

    }

}
